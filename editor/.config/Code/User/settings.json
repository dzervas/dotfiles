{
    "editor.fontFamily": "'Iosevka Term', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "block",
    "files.exclude": {
        "**/.idea": true,
        "**/*.o": true,
        "**/*.pyc": true
    },
    "editor.accessibilitySupport": "off",
    "editor.insertSpaces": false,
    "editor.stablePeek": true,
    "editor.lineNumbers": "relative",
    "editor.parameterHints.cycle": true,
    "editor.renameOnType": true,
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.find.autoFindInSelection": "multiline",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.statusBar.visible": true,
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/*.pyc": true,
        "**/*.pyo": true
    },
    "workbench.list.openMode": "doubleClick",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.scrollToSwitchTabs": true,
    "window.menuBarVisibility": "toggle",
    "explorer.incrementalNaming": "smart",
    "search.quickOpen.includeSymbols": true,
    "search.smartCase": true,
    "search.showLineNumbers": true,
    "search.seedWithNearestWord": true,
    "search.useGlobalIgnoreFiles": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "scm.diffDecorations": "gutter",
    "terminal.external.linuxExec": "kitty",
    "terminal.external.osxExec": "Kitty.app",
    "terminal.external.windowsExec": "C:\\Windows\\System32\\powershell.exe",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "problems.showCurrentInStatus": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.allowForcePush": true,
    "git.defaultCloneDirectory": "~/Lab/",
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.wrapLineLength": 80,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "docker.dockerComposeDetached": false,
    "go.autocompleteUnimportedPackages": true,
    "go.gotoSymbol.includeGoroot": true,
    "go.gotoSymbol.includeImports": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "path-intellisense.extensionOnImport": true,
    "prettier.endOfLine": "auto",
    "prettier.tabWidth": 4,
    "rust.clippy_preference": "on",
    "rust.rust-analyzer.releaseTag": "stable",
    "vim.camelCaseMotion.enable": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visualline": "block-outline",
    "vim.debug.loggingLevelForConsole": "warn",
    "vim.enableNeovim": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.statusBarColorControl": true,
    "vim.timeout": 500,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.visualstar": true,
    "go.useLanguageServer": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: \"Visual Studio\", UseTab: Always }",
    "C_Cpp.updateChannel": "Insiders",
    "explorer.confirmDelete": false,
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "platformio-ide.disablePIOHomeStartup": true,
    "vim.commandLineModeKeyBindingsNonRecursive": [
        { "before": [ "<leader>b" ], "commands": [ "workbench.action.tasks.build" ] },
        { "before": [ "<leader>ga" ], "commands": [ "git.stage" ] },
        { "before": [ "<leader>gb" ], "commands": [ "git.branch" ] },
        { "before": [ "<leader>gc" ], "commands": [ "git.commit" ] },
        { "before": [ "<leader>gc" ], "commands": [ "git.commitAll" ] },
        { "before": [ "<leader>gp" ], "commands": [ "git.push" ] },
        { "before": [ "<leader>gu" ], "commands": [ "git.unstage" ] },
        { "before": [ "<leader>r" ], "commands": [ "code-runner.run" ] },
        { "before": [ "<leader>e" ], "commands": [ "workbench.action.view.problems" ] },
        { "before": [ "<leader><esc>" ], "commands": [ "editor.debug.action.toggleBreakpoint" ] },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        { "before": [ "<leader>ga" ], "commands": [ "git.stageSelectedRanges" ] },
        { "before": [ "<leader>gu" ], "commands": [ "git.unstageSelectedRanges" ] },
    ],
    "json.format.enable": false,
    "workbench.editor.showTabs": true,
}
